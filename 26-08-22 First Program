#include<stdio.h>
int main()
{
    char m[100];
    int uprcount=0,lwrcount=0,numcount=0;
    printf("Enter string:\n");
    scanf("%s",m);
    for(int i=0;m[i]!='\0';i++)
    {
        if(m[i]>=65 && m[i]<=90)
        uprcount++;
        else if(m[i]>=97 && m[i]<=122)
        lwrcount++;
        else if(m[i]>=48 && m[i]<=57)
        numcount++;
    }
    printf("the uppercase count of character is %d\n",uprcount);
    printf("the lowercase count of character is %d\n",lwrcount);
    printf("the numcount count of character is %d\n",numcount);
    return 0;
}#include<stdio.h>
int main()
{
    char m[100];
    int uprcount=0,lwrcount=0,numcount=0;
    printf("Enter string:\n");
    scanf("%s",m);
    for(int i=0;m[i]!='\0';i++)
    {
        if(m[i]>=65 && m[i]<=90)
        uprcount++;
        else if(m[i]>=97 && m[i]<=122)
        lwrcount++;
        else if(m[i]>=48 && m[i]<=57)
        numcount++;
    }
    printf("the uppercase count of character is %d\n",uprcount);
    printf("the lowercase count of character is %d\n",lwrcount);
    printf("the numcount count of character is %d\n",numcount);
    return 0;
}
